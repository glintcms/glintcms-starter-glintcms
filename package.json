{
  "name": "glintcms-starter-glintcms",
  "version": "1.2.2",
  "main": "server.js",
  "browser": "browser.js",
  "scripts": {
    "prepublish": "npm run assets -- -e prod",
    "version": "NODE_ENV=production && npm run assets -- -e prod && npm run build",
    "cleanup": "rm -rf node_modules/ && rm -rf lib/*/node_modules/",
    "assume-unchanged": "git update-index --assume-unchanged datastore/glint/role/7032a580-5c4e-11e5-b149-5bd68197214e.json; git update-index --assume-unchanged datastore/glint/user/7032a580-5c4e-11e5-b149-5bd68197214e.json",
    "install-www": "find $(pwd)/node_modules -type l -maxdepth 1 | xargs -L 1 bash -c 'cd \"$0\" && pwd && npm install'",
    "link-local": "assets-bundler './lib/*' './node_modules/*' -rf",
    "watch": "watchify -t [ envify purge ] -t [brfs] browser.js -o public/build/bundle.js --debug &",
    "minify": "cat public/build/bundle.js | uglifyjs -o public/build/bundle.min.js",
    "assets": "assets-bundler 'node_modules/:module/public' 'public/assets/:module' -rf",
    "setup": "npm install assets-bundler && npm run link-local -- -e prod && npm install && npm run build",
    "setup-www": "npm install assets-bundler && npm run link-local -- -e dev && npm install && npm run install-www && npm run build-www",
    "build": "npm run link-local -- -e prod && npm run assets -- -e prod && browserify -t envify -g [ uglifyify -x '.js' --ignore '**/public/*' ] browser.js -o public/build/bundle.js",
    "build-www": "npm run link-local -- -e dev && npm run assets -- -e dev && browserify browser.js -o public/build/bundle.js --debug",
    "start": "NODE_ENV=production && npm run build && node start",
    "www": "NODE_ENV=development && npm run assets -- -e dev && npm run watch && nodemon -e js,ejs,dot start.js"
  },
  "bundledDependencies": [
    "page-adapter",
    "page-auth",
    "page-auth-access",
    "page-config",
    "page-contact",
    "page-error",
    "page-filemanager",
    "page-is-bot",
    "page-main",
    "page-upload",
    "widget-menu",
    "wrap-layout"
  ],
  "dependencies": {
    "assets-bundler": "^2.0.0",
    "clone": "^1.0.2",
    "compression": "^1.5.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.3.3",
    "debug": "^2.2.0",
    "defaults": "^1.0.2",
    "dot": "^1.0.3",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "glint-adapter": "^1.0.3",
    "glint-adapter-ajax": "^1.0.3",
    "glint-adapter-fs": "^1.0.5",
    "glint-block": "^1.0.2",
    "glint-block-ckeditor": "^1.0.4",
    "glint-block-image": "^1.0.2",
    "glint-block-image-attribute": "^1.0.2",
    "glint-block-markdown": "^1.0.8",
    "glint-block-meta": "^1.0.2",
    "glint-block-text": "^1.0.3",
    "glint-container": "^1.0.2",
    "glint-plugin-adapter-dates": "^1.0.2",
    "glint-plugin-adapter-expires": "^1.0.2",
    "glint-plugin-adapter-id": "^1.0.2",
    "glint-plugin-block-style-editable": "^1.0.2",
    "glint-plugin-wrap-container-place": "^1.0.2",
    "glint-session": "^1.0.2",
    "glint-socket-io": "^1.0.2",
    "glint-tasks": "^1.1.1",
    "glint-trigger": "^1.0.3",
    "glint-trigger-keyboard": "^1.0.2",
    "glint-trigger-sidenav": "^1.0.2",
    "glint-util": "^1.0.2",
    "glint-widget": "^1.0.2",
    "glint-wrap": "^1.0.2",
    "is-browser": "^2.0.1",
    "page-adapter": "file:lib/page-adapter",
    "page-auth": "file:lib/page-auth",
    "page-auth-access": "file:lib/page-auth-access",
    "page-config": "file:lib/page-config",
    "page-contact": "file:lib/page-contact",
    "page-error": "file:lib/page-error",
    "page-filemanager": "file:lib/page-filemanager",
    "page-is-bot": "file:lib/page-is-bot",
    "page-main": "file:lib/page-main",
    "page-upload": "file:lib/page-upload",
    "page.js": "^4.13.3",
    "serve-favicon": "^2.2.0",
    "subarg": "^1.0.0",
    "utils-merge": "^1.0.0",
    "widget-menu": "file:lib/widget-menu",
    "wrap-layout": "file:lib/wrap-layout"
  },
  "devDependencies": {
    "brfs": "^1.3.0",
    "browserify": "^8.1.3",
    "envify": "^3.4.0",
    "tape": "^3.5.0",
    "uglifyify": "^3.0.1",
    "watchify": "^2.4.0"
  },
  "keywords": [
    "glintcms",
    "starter",
    "glint",
    "content management system",
    "cms",
    "website"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glintcms/glintcms-starter-glintcms.git"
  },
  "bugs": {
    "url": "https://github.com/glintcms/glintcms-starter-glintcms/issues"
  },
  "homepage": "https://github.com/glintcms/glintcms-starter-glintcms",
  "author": {
    "name": "Andi Neck"
  },
  "license": "MIT"
}
