<!--contact-->
<div id="contact" class="item {{= it.active['contact'] }}">

  <div class="text-center">
    <h2 class="page-header" data-id="contact-title">
      {{= it['contact-title'] }}
    </h2>
  </div>

  <form class="js-contact-form" method="POST"><input type="hidden" name="_csrf">

    <div class="col-sm-8 col-sm-offset-2">
      <div class="form-group">
        <label for="subject" class="control-label">Subject</label>
        <input class="form-control js-subject" type="subject" name="subject" autofocus="autofocus" placeholder="Inquiry">
      </div>
      <div class="form-group">
        <label for="message" class="control-label">Message</label>
        <textarea class="form-control js-message" type="message" name="message" rows="10" placeholder="Please get back to me."></textarea>
      </div>
      <div class="form-group">
        <a type="submit" class="js-contact-button btn btn-transparent" href="#">
          <span class="glyphicon glyphicon-envelope"></span>Send Email
        </a>
      </div>

    </div>
  </form>

  <div class="clearfix"></div>

</div>
<!--/contact-->

