<!--home-->
<div id="home" class="item {{= it.active['home'] }}">

  <!--home-header-->
  <div class="row">

    <div class="col-md-6">
      <h2 class="page-header" data-id="home-title">{{= it['home-title'] }}</h2>

      <div class="lead" data-id="home-subtitle">
        {{= it['home-subtitle'] }}
      </div>

    </div>

    <div class="col-md-6">
      <div data-id="home-teaser">{{= it['home-teaser']}}</div>
    </div>

  </div>
  <!--/home-header-->

  <!--home-content-->
  <div class="row">

    <div class="divider clearfix"></div>

    <!--home-content-left-->
    <div class="col-sm-6">

      <div class="box" data-id="home-box-1">
        {{= it['home-box-1'] }}
      </div>

      <div class="box" data-id="home-box-2">
        {{= it['home-box-2'] }}
      </div>

      <div class="box hidden" data-id="home-box-3">
        {{= it['home-box-3'] }}
      </div>

    </div>

    <!--home-content-right-->
    <div class="col-sm-6">

      <div class="box" data-id="home-box-4">
        {{= it['home-box-4'] }}
      </div>

      <div class="box" data-id="home-box-5">
        {{= it['home-box-5'] }}
      </div>

      <div class="box" data-id="home-box-6">
        {{= it['home-box-6'] }}
      </div>

    </div>

  </div>

</div>
<!--/home-content-->

<!--works-with-what-->
<div id="works-with-what" class="item {{= it.active['works-with-what'] }}">

  <div class="text-center">
